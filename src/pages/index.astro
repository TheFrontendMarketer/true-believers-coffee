---
import Layout from "../layouts/BaseLayout.astro";

import { Image } from "astro:assets";
import homepageFirstImage from "../assets/homepage_first_image.jpg";
import favOne from "../assets/410x506_light_coffee.jpg";
import favTwo from "../assets/410x506_med_coffee.jpg";
import favThree from "../assets/410x506_dark_coffee.jpg";
import favFour from "../assets/410x506_cold_brew_coffee.jpg";
import favFive from "../assets/410x506_decaf_coffee.jpg";
import favSix from "../assets/410x506_french_roast_coffee.jpg";
import testimonialOne from "../assets/jasmine.jpg";
import testimonialTwo from "../assets/marco.jpg";
import testimonialThree from "../assets/sara.jpg";
import teamMemberOne from "../assets/500x600_alice_johnson.jpg";
import teamMemberTwo from "../assets/480x600_mark.jpg";
import teamMemberThree from "../assets/480x600_sara.jpg";
import teamMemberFour from "../assets/480x600_tom.jpg";
import teamMemberFive from "../assets/480x600_emily.jpg";
import teamMemberSix from "../assets/480x600_james2.jpg";
---

<Layout title="True Believers Coffee" description="True Believers Coffee">
  <main>
    <section
      class="relative min-h-screen flex items-center justify-center md:justify-start md:pb-40"
    >
      <!-- Background Image with Picture tag -->
      <div class="absolute inset-0 z-0">
        <picture>
          <!-- Mobile: Smaller image for faster loading -->
          <source
            srcset="/images/coffee_shop2_hero_mobile.webp"
            media="(max-width: 767px)"
            type="image/webp"
          />
          <!-- Desktop: Full size image -->
          <source
            srcset="/images/coffee_shop2_hero.webp"
            media="(min-width: 768px)"
            type="image/webp"
          />
          <!-- Fallback: Mobile-first approach -->
          <img
            src="/images/coffee_shop2_hero_mobile.jpg"
            alt="Coffee shop interior with warm lighting and customers enjoying artisanal coffee"
            class="w-full h-full object-cover"
            loading="eager"
            fetchpriority="high"
          />
        </picture>
      </div>

      <!-- Content -->
      <div class="site-container w-full text-white relative z-10">
        <h1
          class="text-4xl md:text-[5rem] leading-[110%] mb-6 md:w-[32rem] md:pt-40"
        >
          Experience the Art of Specialty Coffee
        </h1>
        <p class="text-lg mb-8 flex flex-wrap md:w-[32rem]">
          Welcome to our artisanal coffee haven, where every cup tells a story.
          Join us in celebrating sustainable sourcing and the craft of roasting,
          all within a vibrant community space.
        </p>
        <button
          class="bg-white text-black px-8 py-4 rounded-lg hover:bg-gray-100 transition-colors min-h-[44px] min-w-[44px]"
          title="Learn More About True Believers Coffee"
        >
          <a href="/about">Learn More About Us</a>
        </button>
      </div>
    </section>

    <section class="bg-coffee-primary text-white">
      <div class="site-container md:flex md:items-center md:gap-8">
        <div class="md:flex-1">
          <h2 class="text-3xl leading-[110%] pt-16 pb-5">
            Coffee That Tells a Story
          </h2>
          <p>
            Experience the meticulous journey of our coffee, from sustainable
            sourcing to expert roasting. Each cup tells a story of passion and
            quality.
          </p>
          <div class="flex flex-col md:flex-row gap-6">
            <div>
              <p class="text-xl font-bold pt-8">Our Process</p>
              <p>
                Handpicked beans, roasted with care, brewed to perfection for
                you.
              </p>
            </div>
            <div>
              <p class="text-xl font-bold pt-8">Meet the Artisans</p>
              <p>
                Drop in and meet the talented individuals behind your favorite
                brews and their stories.
              </p>
            </div>
          </div>
        </div>

        <div class="pt-14 pb-16 md:flex-1">
          <Image
            src={homepageFirstImage}
            alt="Coffee beans and brewing equipment showcasing our artisanal coffee process"
            class="rounded-2xl"
            widths={[362, 540, 720, 1080]}
            sizes="(max-width: 768px) 362px, (max-width: 1024px) 540px, 720px"
            loading="lazy"
            format="webp"
          />
        </div>
      </div>
    </section>

    <section class="bg-white">
      <div class="site-container">
        <div class="text-center">
          <p class="pt-16 md:pt-28">Brewed</p>
          <h2 class="text-3xl pt-3 md:pt-4">
            Crafting Coffee with Heart and Purpose
          </h2>
          <p class="pt-6">
            Our commitment to sustainability starts at the source. We partner
            with farmers who prioritize eco-friendly practices to bring you the
            finest beans.
          </p>
        </div>
        <div class="flex flex-col md:flex-row pt-12 md:pt-20">
          <div class="flex flex-col items-center justify-start flex-1">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="49"
              height="49"
              viewBox="0 0 49 49"
              fill="none"
            >
